{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".micromamba/": true,
    "**/data/**": true,
    "**/exp/**": true,
  },
  "search.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".micromamba/": true,
    "**/data/**": true,
    "**/exp/**": true,
  },
  "python.analysis.exclude": [
    "**/data/**",
    "**/exp/**",
  ],
  "python.analysis.extraPaths": [
    "./prep"
  ],
  "notebook.output.textLineLimit": 50,
  "python.analysis.typeCheckingMode": "off"
}